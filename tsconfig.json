{
    "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "ES2019",
    "esModuleInterop": true,
    "allowJs": true,
    "strictNullChecks": true,
    "baseUrl": "packages",
    "paths": {
      "@/*": ["*"],
      "@api/*": ["api/src/*"],
      "@framework": ["framework/src/*"]
    }
  },
  "include": ["packages", "tests"],
  "exclude": []
}